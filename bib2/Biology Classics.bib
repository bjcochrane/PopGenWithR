}
@article{Bhupathy2016908,
abstract = {We update the distribution of the little known Captain's Wood Snake (Xylophis captaini) in the Western Ghats, based on new observations and collation of the literature. The Maximum Entropy (MaxEnt) algorithm was used to predict the distribution of the species, which showed potential sites south of 10°N and elevations between 50 and 1000 m amsl. Xylophis captaini is listed as 'Least Concern' under IUCN cricriteria, and we suggest the possible elevation of its status to the 'Near-Threatened' category on account of its narrow distributional range and general lack of data on its ecology. The present study highlights the utility of niche models in assessing the distribution of cryptic and little known species in biodiversity-rich areas such as the Western Ghats.},
