journal = {Annales Zoologici Fennici},
number = {4},
pages = {349--370},
title = {{Evaluating habitat suitability for the middle spotted woodpecker using a predictive modelling approach}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84906233404{\&}doi=10.5735{\%}2F086.051.0402{\&}partnerID=40{\&}md5=a43d61b2c37c13e17a980e4a87330ea2},
volume = {51},
