doi = {10.4003/006.031.0115},
journal = {American Malacological Bulletin},
number = {1},
pages = {39--50},
title = {{The giant African snail, Achatina fulica (Gastropoda: Achatinidae): Using bioclimatic models to identify south American areas susceptible to invasion}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84874078071{\&}doi=10.4003{\%}2F006.031.0115{\&}partnerID=40{\&}md5=8c09383aac16a02605e6e1f6dd6f1506},
