title = {{Predicting geographic distributions of Phacellodomus species (Aves: Furnariidae) in South America based on ecological niche modeling [Predi{\c{c}}{\~{a}}o das distribui{\c{c}}{\~{o}}es geogr{\'{a}}ficas das esp{\'{e}}cies de Phacellodomus (Aves: Furnariidae) na Am{\'{e}}rica do Sul utilizando mod}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84907319219{\&}doi=10.4025{\%}2Factascibiolsci.v36i3.22165{\&}partnerID=40{\&}md5=e105b1289266338b0fb2c518e99d44e9},
