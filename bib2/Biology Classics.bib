doi = {10.1590/0074-02760140462},
journal = {Memorias do Instituto Oswaldo Cruz},
number = {4},
pages = {551--559},
title = {{Geographic distribution of phlebotomine sandfly species (Diptera: Psychodidae) in central-west brazil}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84930837146{\&}doi=10.1590{\%}2F0074-02760140462{\&}partnerID=40{\&}md5=2b42c7a00f7671f6258690c25edee287},
volume = {110},
