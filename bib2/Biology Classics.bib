number = {9},
pages = {2960--2966},
title = {{The potential distribution and population protection priority of amygdalus mongolica}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84929448039{\&}doi=10.5846{\%}2Fstxb201308292173{\&}partnerID=40{\&}md5=b43bbd7201cae2ab13b1972d9ac81f64},
volume = {35},
