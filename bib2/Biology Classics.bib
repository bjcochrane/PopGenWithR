journal = {Revista Chapingo, Serie Ciencias Forestales y del Ambiente},
number = {2},
pages = {187--201},
title = {{Selection of environmental predictors for species distribution modeling in Maxent [Selecci{\'{o}}n de predictores ambientales para el modelado de la distribuci{\'{o}}n de especies en Maxent]}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84907818795{\&}doi=10.5154{\%}2Fr.rchscfa.2013.09.034{\&}partnerID=40{\&}md5=84170bc15d49b1d424eea5bdba3e4682},
volume = {20},
