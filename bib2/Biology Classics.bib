author = {Simon, R and Xie, C H and Clausen, A and Jansky, S H and Halterman, D and Conner, T and Knapp, S and Brundage, J and Symon, D and Spooner, D},
doi = {10.1614/IPSM-D-09-00043.1},
journal = {Invasive Plant Science and Management},
number = {3},
pages = {286--293},
title = {{Wild and cultivated potato (Solanum sect. Petota) escaped and persistent outside of its natural range}},
