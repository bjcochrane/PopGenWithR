pages = {6--21},
title = {{Forest Inventories and habitat models to predict regeneration of Mediterranean woody species in forest plantations [Integraci{\'{o}}n de datos de inventario y modelos de h{\'{a}}bitat para predecir la regeneraci{\'{o}}n de especies le{\~{n}}osas mediterr{\'{a}}neas en repoblaciones fo}},
