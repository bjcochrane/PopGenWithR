@article{Oraie2014125,
abstract = {Ophisops elegans, a common lizard with a wide distribution range in Iran, was selected to investigate the influence of environmental factors on its distribution pattern. Based on a distribution model developed with the software Maxent for O. elegans, the most important factors influencing the distribution pattern were found to be high winter precipitation, intermediate levels of Normalized Difference Vegetation Index (NDVI) and intermediate levels of sunshine. It seems that overall plant cover and competition with Mesalina watsonana are the main factors which influence the distribution pattern of O. elegans in the central Iranian Plateau. {\textcopyright} 2014 {\textcopyright} 2014 Taylor {\&} Francis.},
