journal = {International Journal of Zoological Research},
number = {1},
pages = {9--14},
title = {{Changes in the distribution of lesser adjutant storks (Leptoptilos javanicus) in south and southeast asia: A plausible evidence of global climate and land-use change effect}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84898873993{\&}doi=10.3923{\%}2Fijzr.2014.9.14{\&}partnerID=40{\&}md5=9ca0bd839e95c4acab165806f2ab44be},
volume = {10},
