pages = {230--238},
title = {{Modeling of Andean bear Tremarctos ornatus distribution in the dry forest of Mara{\~{n}}{\'{o}}n (Peru) [Modelaci{\'{o}}n de la distribuci{\'{o}}n del oso andino Tremarctos ornatus en el bosque seco del Mara{\~{n}}{\'{o}}n (Per{\'{u}})]}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84961815110{\&}doi=10.1016{\%}2Fj.rmb.2016.01.008{\&}partnerID=40{\&}md5=565ce1994e2c50985965ffe6417d7762},
volume = {87},
