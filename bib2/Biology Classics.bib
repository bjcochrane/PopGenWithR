pages = {587--598},
title = {{Current and potential distribution of Taxus globosa (Taxaceae) in Mexico [Distribuci{\'{o}}n Actual y Potencial de Taxus Globosa (Taxaceae) en M{\'{e}}xico]}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84870887969{\&}partnerID=40{\&}md5=1352335b792dc63e1c87654c34092bb4},
volume = {6},
