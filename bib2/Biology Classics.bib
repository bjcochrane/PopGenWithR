title = {{Potential distribution model of Pinaceae species under climate change scenarios in Michoac{\'{a}}n [Modelado de la distribuci{\'{o}}n potencial de especies de Pinaceae bajo escenarios de cambio clim{\'{a}}tico en Michoac{\'{a}}n]}},
