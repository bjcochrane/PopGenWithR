journal = {Revista Mexicana de Biodiversidad},
number = {1},
pages = {170--183},
title = {{Diversity and distribution of Marcgraviaceae in Mexico [Diversidad y distribuci{\'{o}}n de Marcgraviaceae en M{\'{e}}xico]}},
