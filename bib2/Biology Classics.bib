doi = {10.1016/j.rmb.2017.03.001},
journal = {Revista Mexicana de Biodiversidad},
number = {2},
pages = {415--424},
title = {{Potential distribution of native freshwater fish in Tabasco, Mexico [Distribuci{\'{o}}n potencial de los peces nativos de agua dulce de Tabasco, M{\'{e}}xico]}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85018368192{\&}doi=10.1016{\%}2Fj.rmb.2017.03.001{\&}partnerID=40{\&}md5=5b587791f0c3ce6efafcc9657921765f},
volume = {88},
