booktitle = {Freshwater Crayfish},
doi = {10.5869/fc.2013.v19-2.189},
number = {2},
pages = {189--195},
title = {{Modelling environmental niche for the endangered crayfish Austropotamobius pallipes complex in northern and central Italy}},
