doi = {10.1017/S0007485309006713},
journal = {Bulletin of Entomological Research},
number = {1},
pages = {35--48},
title = {{Ecological niche and potential geographic distribution of the invasive fruit fly Bactrocera invadens (Diptera, Tephritidae)}},
