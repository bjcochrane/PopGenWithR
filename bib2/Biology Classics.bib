author = {de Zaburl{\'{i}}n, N and Vogler, R E and Molina, M J and Llano, V M},
doi = {10.1111/jpy.12382},
journal = {Journal of Phycology},
number = {2},
pages = {200--208},
title = {{Potential distribution of the invasive freshwater dinoflagellate Ceratium furcoides (Levander) Langhans (Dinophyta) in South America}},
