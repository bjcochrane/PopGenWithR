pages = {351--7},
pmid = {19711245},
title = {{Epigenetics: definition, mechanisms and clinical perspective.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19711245 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2791696},
volume = {27},
