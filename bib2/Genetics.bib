title = {{Shaping mitotic chromosomes: From classical concepts to molecular mechanisms.}},
url = {http://doi.wiley.com/10.1002/bies.201500020 http://www.ncbi.nlm.nih.gov/pubmed/25988527},
volume = {37},
year = {2015}
}
