number = {6213},
pages = {1258096--1258096},
pmid = {25430774},
title = {{The new frontier of genome engineering with CRISPR-Cas9}},
url = {http://www.sciencemag.org/content/346/6213/1258096.long},
volume = {346},
year = {2014}
}
