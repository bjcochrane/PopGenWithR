journal = {Genes {\&} development},
month = {aug},
number = {16},
pages = {1831--42},
pmid = {19684108},
publisher = {Cold Spring Harbor Laboratory Press},
title = {{Lessons from X-chromosome inactivation: long ncRNA as guides and tethers to the epigenome.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19684108 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2725936},
